        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Any area, discipline, subject of knowledge. E.g. algebra, avionics, ballet, theology, engineering, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/ont/d0.owl"/>
        <rdfs:label xml:lang="en">topic</rdfs:label>
    </owl:Class>
    


